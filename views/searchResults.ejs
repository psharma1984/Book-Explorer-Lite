<%- include("partials/head.ejs") %>
<%- include("partials/header.ejs") %>
<h2>Search Results for <%= query %></h2>

<% if (results.length > 0) { %>
    <ul>
        <% results.forEach((result) => { %>
            <li><%= result.title %></li>
        <% }) %>
    </ul>
<% } else { %>
    <p>No results found</p>
<% } %>
